{
  "kind": "collectionType",
  "collectionName": "productos",
  "info": {
    "singularName": "producto",
    "pluralName": "productos",
    "displayName": "Producto"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "codigo": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "nombre"
    },
    "nombre": {
      "type": "string",
      "required": true
    },
    "precioCompra": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "precioVenta": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "existencia": {
      "type": "integer",
      "required": true
    },
    "activo": {
      "type": "boolean",
      "default": true
    },
    "empresa": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::empresa.empresa",
      "required": true
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "required": true
    }
  }
}
