{
  "kind": "collectionType",
  "collectionName": "inventario_movimientos",
  "info": {
    "singularName": "inventario-movimiento",
    "pluralName": "inventario-movimientos",
    "displayName": "InventarioMovimiento"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "producto": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::producto.producto"
    },
    "empresa": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::empresa.empresa"
    },
    "sucursal": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::sucursal.sucursal"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "cantidad": {
      "type": "decimal",
      "required": true
    },
    "tipoMovimiento": {
      "type": "enumeration",
      "default": "ENTRADA",
      "enum": [
        "ENTRADA",
        "SALIDA",
        "AJUSTE"
      ]
    },
    "comentario": {
      "type": "string"
    }
  }
}
